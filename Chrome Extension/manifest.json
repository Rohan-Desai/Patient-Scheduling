{
  "manifest_version": 3,
  "name": "Patient Scheduler PatientSync",
  "description": "Base Level Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "webNavigation", "webRequest"],
  "content_scripts": [
    {
      "matches": ["https://kinnser.net/AM/hotbox*"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/get_started16.png"
  }
  
}